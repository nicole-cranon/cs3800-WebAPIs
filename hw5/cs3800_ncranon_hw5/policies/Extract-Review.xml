<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="Extract-Review">
    <DisplayName>Extract Review</DisplayName>
    <FaultRules/>
    <Properties/>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <JSONPayload>
      <Variable name="mtype">
          	<JSONPath>$.type</JSONPath>
      </Variable>
      <Variable name="movieId">
          	<JSONPath>$.movieId</JSONPath>
      </Variable>
      <Variable name="reviewer">
            <JSONPath>$.reviewer</JSONPath>
        </Variable>
      <Variable name="comment">
            <JSONPath>$.comment</JSONPath>
        </Variable>
      <Variable name="rating">
            <JSONPath>$.rating</JSONPath>
        </Variable>
    </JSONPayload>
    <Source clearPayload="false">request</Source>
</ExtractVariables>

